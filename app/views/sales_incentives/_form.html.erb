<%= simple_form_for(@sales_incentive) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :min_value %>
    <%= f.input :max_value %>
    <%= f.input :commission %>
    <%= f.input :no_of %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
