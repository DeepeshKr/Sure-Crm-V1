<h1>Listing product_active_codes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sortorder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_active_codes.each do |product_active_code| %>
      <tr>
        <td><%= product_active_code.name %></td>
        <td><%= product_active_code.sortorder %></td>
        <td><%= link_to 'Show', product_active_code %></td>
        <td><%= link_to 'Edit', edit_product_active_code_path(product_active_code) %></td>
        <td><%= link_to 'Destroy', product_active_code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product active code', new_product_active_code_path %>
