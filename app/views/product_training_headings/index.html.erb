<h1>Listing product_training_headings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sortorder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_training_headings.each do |product_training_heading| %>
      <tr>
        <td><%= product_training_heading.name %></td>
        <td><%= product_training_heading.sortorder %></td>
        <td><%= link_to 'Show', product_training_heading %></td>
        <td><%= link_to 'Edit', edit_product_training_heading_path(product_training_heading) %></td>
        <td><%= link_to 'Destroy', product_training_heading, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product training heading', new_product_training_heading_path %>
