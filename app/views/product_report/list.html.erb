<h1>ProductReport#list</h1>
<p>Find me in app/views/product_report/list.html.erb</p>
<div class="row">
 <%= form_tag("/productdetails", method: "get") do  %>
<div class="col-xs-4">
<%= select_tag(:productmaster_id, 
                options_from_collection_for_select(@productmasterlist, :id, :name), 
                {:class => "form-control"} )  %>
</div>
<div class="col-xs-4">
<div class="input-daterange input-group">
<%= text_field_tag(:from_date, nil, :data => {:provide => "datepicker"}, :class => "form-control", :required => "true" )   %>
<span class="input-group-addon">to</span>
<%= text_field_tag(:to_date, nil, :data => {:provide => "datepicker"}, :class => "form-control", :required => "true" )  %>
</div>
</div>
<div class="col-xs-2">
	<%= submit_tag "Report", name: nil, class: "btn btn-primary" %>
</div>
<% end %>
</div>

<hr/>
