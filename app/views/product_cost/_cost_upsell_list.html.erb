<h4>Product Costs (UPSELL)</h4>
<% if @ropupsprod.present? %>
<p class="lead">Cost: <%= @ropupsprod.first.cost  %>

<br>
Revenue: <%= @ropupsprod.first.sp  %>

 </p>


<table class="table table-hover">
  <thead>
    <tr>
      <th>PROD</th>
      <th>COST</th>
      <th>COMM</th>
      <th>SP</th>
      <th>POSTAGE</th>
      <th>SHIP</th>
    </tr>
  </thead>

  <tbody>
    <% @ropupsprod.each do |d| %>
      <tr>
        <td><%= d.prod %></td>
        <td><%= d.cost %></td>
        <td><%= d.comm %></td>
        <td><%= d.sp %></td>
        <td><%= d.postage %></td>
        <td><%= d.ship %></td>

      </tr>
    <% end %>
  </tbody>
</table>
<% else %>
<h3> No products found in Up Sell Cost</h3>
<% end %>