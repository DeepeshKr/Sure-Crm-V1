<h4>Product Costs <%= @ropmasters.first.totalcost %> &nbsp; Revenue: <%=@ropmasters.first.totalrevenue %></h4>

<table class="table table-hover">
  <thead>
    <tr>
       <td>prod</td>
           <td>PC1 <br>     PC2 <br> PC3 </td>
       <td>BP1 <br>      BP2  <br>      BP3 </td>
       <td>SH1 <br>     SH2  <br>      SH3 </td>
       <td>VC1  <br>      VC2  <br>      VC3   </td>
       <td>TC1  <br>    TC2  <br>      TC3   </td>
       <td>PACK1   <br>     PACK2 <br>      PACK3   </td>
       <td>WBP <br> WVC <br> PCB1 </td>
       <td>BPB1 <br> SHB1 <br> VCB1 </td>
       <td>TCB1 <br> PCP1 <br> BPP1 </td>
       <td>SHP1 <br>VCP1 <br> TCP1 </td>
       <td>PCM1 <br>BPM1 <br> SHM1 </td>
       <td>VCM1 <br>TCM1 <br> DNBASIC </td>
       <td>WPC <br>ROYALTY <br> COPRET </td>
    </tr>
  </thead>

  <tbody>
    <% @ropmasters.each do |d| %>
      <tr>
        <td><%= d.prod %></td>
           <td><%= d.pc1 %> <br>       <%= d.pc2 %><br>       <%= d.pc3 %></td>
       <td><%= d.bp1 %> <br>      <%= d.bp2 %> <br>      <%= d.bp3 %></td>
       <td><%= d.sh1 %> <br>      <%= d.sh2 %> <br>       <%= d.sh3 %></td>
       
       <td><%= d.vc1 %>  <br>      <%= d.vc2 %> <br>       <%= d.vc3 %>  </td>
        <td><%= d.tc1 %>  <br>      <%= d.tc2 %> <br>       <%= d.tc3 %>  </td>

           <td><%= d.pack1 %>  <br>      <%= d.pack2 %> <br>       <%= d.pack3 %>  </td>
        <td><%= d.wbp %> <br> <%= d.wvc %> <br> <%= d.pcb1 %></td>
       <td><%= d.bpb1 %> <br> <%= d.shb1 %> <br> <%= d.vcb1 %></td>
       <td><%= d.tcb1 %><br><%= d.pcp1 %><br><%= d.bpp1 %></td>
       <td><%= d.shp1 %><br><%= d.vcp1 %><br><%= d.tcp1 %></td>
       <td><%= d.pcm1 %><br><%= d.bpm1 %><br><%= d.shm1 %></td>
       <td><%= d.vcm1 %><br><%= d.tcm1 %><br><%= d.dnbasic %></td>
      <td><%= d.wpc %><br><%= d.royalty %><br><%= d.copret %></td>

      </tr>
    <% end %>
  </tbody>
</table>

