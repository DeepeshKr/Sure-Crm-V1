<br>
<div class="digg_pagination">
    <%= will_paginate @distributor_pincode_lists, :container => false %>
</div>
<br>


<table class="table table-hover">
  <thead>
    <tr>
      <th>State</th>
      <th>City</th>
      <th>Locality</th>
      <th>Sort order</th>
      <th>Pincode</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @distributor_pincode_lists.each do |distributor_pincode_list| %>
      <tr>
        <td><%= distributor_pincode_list.state %></td>
        <td><%= distributor_pincode_list.city %></td>
        <td><%= distributor_pincode_list.locality %></td>
        <td><%= distributor_pincode_list.sort_order %></td>
        <td><%= distributor_pincode_list.pincode %></td>
        <td><%= link_to 'Edit', edit_distributor_pincode_list_path(distributor_pincode_list) %></td>
        <td><%= link_to 'Destroy', distributor_pincode_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>