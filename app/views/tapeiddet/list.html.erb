
<h3>Tape Ids </h3>


<table class="table table-hover">
  <thead>
    <tr>
      <th>Channel</th>
      <th>Prod</th>
      <th>Basic</th>
      <th>Postage</th>
      <th>Duration</th>
      <th>Remarks</th>
      <th>Tape Id</th>
      <th>Sr No</th>
      <th>T Date</th>
      <th>E Name</th>
    </tr>
  </thead>

  <tbody>
    <% @tapeids.each do |d| %>
      <tr>
        <td><%= d.channel %></td>
       <td><%= d.prod %></td>
       <td><%= d.basic %></td>
       <td><%= d.postage %></td>
       <td><%= d.duration %></td>
       <td><%= d.remarks %></td>
       <td><%= d.tapeid %></td>
       <td><%= d.srno %></td>   
       <td><%= d.tdate %></td>   
       <td><%= d.ename %></td>   
      </tr>
    <% end %>
  </tbody>
</table>


