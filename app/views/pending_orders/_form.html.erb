<%= simple_form_for(@pending_order) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :order_ref_id %>
    <%= f.input :order_no %>
    <%= f.input :order_dispatch_status_id %>
    <%= f.input :cod_amount %>
    <%= f.input :cod_amount %>
    <%= f.input :pay_u_amount %>
    <%= f.input :pay_u_amount %>
    <%= f.input :courier_list_id %>
    <%= f.input :pay_u_status_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
