<p id="notice"><%= notice %></p>

<h1>Listing Distributor Stock Books</h1>

<table>
  <thead>
    <tr>
      <th>Corporate</th>
      <th>Product master</th>
      <th>Product variant</th>
      <th>Product list</th>
      <th>Prod</th>
      <th>Opening qty</th>
      <th>Opening value</th>
      <th>Sold qty</th>
      <th>Sold value</th>
      <th>Return qty</th>
      <th>Return value</th>
      <th>Closing qty</th>
      <th>Closing value</th>
      <th>Book date</th>
      <th>List barcode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @distributor_stock_books.each do |distributor_stock_book| %>
      <tr>
        <td><%= distributor_stock_book.corporate_id %></td>
        <td><%= distributor_stock_book.product_master_id %></td>
        <td><%= distributor_stock_book.product_variant_id %></td>
        <td><%= distributor_stock_book.product_list_id %></td>
        <td><%= distributor_stock_book.prod %></td>
        <td><%= distributor_stock_book.opening_qty %></td>
        <td><%= distributor_stock_book.opening_value %></td>
        <td><%= distributor_stock_book.sold_qty %></td>
        <td><%= distributor_stock_book.sold_value %></td>
        <td><%= distributor_stock_book.return_qty %></td>
        <td><%= distributor_stock_book.return_value %></td>
        <td><%= distributor_stock_book.closing_qty %></td>
        <td><%= distributor_stock_book.closing_value %></td>
        <td><%= distributor_stock_book.book_date %></td>
        <td><%= distributor_stock_book.list_barcode %></td>
        <td><%= link_to 'Show', distributor_stock_book %></td>
        <td><%= link_to 'Edit', edit_distributor_stock_book_path(distributor_stock_book) %></td>
        <td><%= link_to 'Destroy', distributor_stock_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Distributor stock book', new_distributor_stock_book_path %>
