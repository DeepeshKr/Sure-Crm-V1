<%= simple_form_for(@cable_opertor_comm) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :order_no %>
    <%= f.input :order_date %>
    <%= f.input :channel %>
    <%= f.input :product %>
    <%= f.input :amount %>
    <%= f.input :customer_name %>
    <%= f.input :city %>
    <%= f.input :comm %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
