<%= simple_form_for(@product_test_ppo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :prod_code %>
    <%= f.input :barcode %>
    <%= f.input :basic_price %>
    <%= f.input :shipping %>
    <%= f.input :channel %>
    <%= f.input :aired_date %>
    <%= f.input :slot %>
    <%= f.input :orders %>
    <%= f.input :ppo %>
    <%= f.input :ad_cost %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
