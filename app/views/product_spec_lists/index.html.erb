<p id="notice"><%= notice %></p>

<h1>Listing Product Spec Lists</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Spec 1</th>
      <th>Spec 2</th>
      <th>Spec 3</th>
      <th>Spec 4</th>
      <th>Spec 5</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_spec_lists.each do |product_spec_list| %>
      <tr>
        <td><%= product_spec_list.name %></td>
        <td><%= product_spec_list.spec_1 %></td>
        <td><%= product_spec_list.spec_2 %></td>
        <td><%= product_spec_list.spec_3 %></td>
        <td><%= product_spec_list.spec_4 %></td>
        <td><%= product_spec_list.spec_5 %></td>
        <td><%= product_spec_list.description %></td>
        <td><%= link_to 'Show', product_spec_list %></td>
        <td><%= link_to 'Edit', edit_product_spec_list_path(product_spec_list) %></td>
        <td><%= link_to 'Destroy', product_spec_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product spec list', new_product_spec_list_path,  class: "btn btn-info" %>
