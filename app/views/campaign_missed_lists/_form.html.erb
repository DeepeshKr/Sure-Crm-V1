<%= simple_form_for(@campaign_missed_list) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :product_list_id %>
    <%= f.input :product_variant_id %>
    <%= f.input :productmaster_id %>
    <%= f.input :external_prod %>
    <%= f.input :reason %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
