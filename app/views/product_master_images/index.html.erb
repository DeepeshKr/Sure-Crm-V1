<p id="notice"><%= notice %></p>

<h1>Listing Product Master Images</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Sort order</th>
      <th>Prod</th>
      <th>Barcode</th>
      <th>Product variant</th>
      <th>Product list</th>
      <th>Product master</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_master_images.each do |product_master_image| %>
      <tr>
        <td><%= product_master_image.name %></td>
        <td><%= product_master_image.description %></td>
        <td><%= product_master_image.sort_order %></td>
        <td><%= product_master_image.prod %></td>
        <td><%= product_master_image.barcode %></td>
        <td><%= product_master_image.product_variant_id %></td>
        <td><%= product_master_image.product_list_id %></td>
        <td><%= product_master_image.product_master_id %></td>
        <td><%= link_to 'Show', product_master_image %></td>
        <td><%= link_to 'Edit', edit_product_master_image_path(product_master_image) %></td>
        <td><%= link_to 'Destroy', product_master_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product master image', new_product_master_image_path %>
