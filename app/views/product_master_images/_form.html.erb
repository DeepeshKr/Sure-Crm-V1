<%= simple_form_for(@product_master_image) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :sort_order %>
    <%= f.input :prod %>
    <%= f.input :barcode %>
    <%= f.input :product_variant_id %>
    <%= f.input :product_list_id %>
    <%= f.input :product_master_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
