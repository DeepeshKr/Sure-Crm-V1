<%= simple_form_for(@distributor_missed_pincode) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :pincode %>
    <%= f.input :no_of_orders %>
    <%= f.input :total_value %>
    <%= f.input :last_ran_on %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
