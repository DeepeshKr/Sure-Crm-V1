<table class="table table-hover" >
  <thead>
    <tr>
      
      
      <th>Price</th>
      <th>Taxes</th>
      <th>Total</th>
      
     <th>Sell Type</th>
      <th>Activeid</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_variants.each do |product_variant| %>
      <tr>    
        <td><%= product_variant.price %></td>
        <td><%= product_variant.taxes %></td>
        <td><%= product_variant.total %></td>
       
       <td> <%= product_variant.product_sell_type.name if product_variant.product_sell_type %> </td>
        <td><%= product_variant.product_master.product_active_code.name if product_variant.product_master.product_active_code %></td>
        <td><%= link_to 'Show', product_variant %></td>
        <td><%= link_to 'Edit', edit_product_variant_path(product_variant) %></td>
      </tr>
    <% end %>
  </tbody>
</table>


