 <div class="row">
	 <div class="col-xs-6">

	 <h4>Hourly Orders</h4>
	 </div>
	 <div class="col-xs-6">

	 		
	</div>
 </div>

<% if @employeeorderlist.present? %>
<table class="table table-hover table-bordered">
  <thead>
    <tr>
     <th class="text-right">No</th>
     <th class="text-right">Start Time</th>
     <th class="text-right">End Time</th>
     <th class="text-right">CC Orders</th>
       <th class="text-right">CC Value</th>
      <th class="text-right">COD Orders</th>
       <th class="text-right">COD Value</th>
       <th class="text-right">Total Orders</th>
     <th class="text-right">Total Value</th>
    <th></th>
     </tr>
  </thead>
<tbody>
   <% @employeeorderlist.each do |c| %>
   <tr>
   <td><%= @sno %></td>

 <td><%= c[:starttime] %></td>
 <td><%= c[:endtime] %></td>
<td class="text-right">
   <%= c[:ccorders] %> 
</td>
<td class="text-right">
   <%= c[:ccvalue] %> 
</td>
<td class="text-right">
   <%= c[:codorders] %> 
</td>
<td class="text-right">
   <%= c[:codvalue] %> 
</td>

<td class="text-right">
   <%= c[:nos] %> 
</td>
<td class="text-right">
   <%= c[:total] %> 
</td>
<td >
<%= link_to "View", orders_list_path(for_date: @or_for_date, start_time: c[:start_time], end_time: c[:end_time]), :target => "_blank" %>
</td>
  </tr> 
  <% @sno += 1 %> 
 <% end %>

 

  </tbody>

  </table>
  <% else %>
  <div class="well well-sm">
  <h5>Please select a date to generate the report </h5>
  </div>
  <% end %>
 <br>


<%= @hourlist %>