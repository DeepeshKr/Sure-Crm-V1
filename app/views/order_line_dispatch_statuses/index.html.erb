<h1>Listing order_line_dispatch_statuses</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Sortorder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @order_line_dispatch_statuses.each do |order_line_dispatch_status| %>
      <tr>
        <td><%= order_line_dispatch_status.name %></td>
        <td><%= order_line_dispatch_status.sortorder %></td>
        <td><%= link_to 'Show', order_line_dispatch_status %></td>
        <td><%= link_to 'Edit', edit_order_line_dispatch_status_path(order_line_dispatch_status) %></td>
        <td><%= link_to 'Destroy', order_line_dispatch_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order line dispatch status', new_order_line_dispatch_status_path %>
