<p id="notice"><%= notice %></p>

<h1>Listing Product Stock Adjusts</h1>

<table>
  <thead>
    <tr>
      <th>Product master</th>
      <th>Product list</th>
      <th>Change stock</th>
      <th>Ext prod code</th>
      <th>Barcode</th>
      <th>Created date</th>
      <th>Emp code</th>
      <th>Emp</th>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_stock_adjusts.each do |product_stock_adjust| %>
      <tr>
        <td><%= product_stock_adjust.product_master_id %></td>
        <td><%= product_stock_adjust.product_list_id %></td>
        <td><%= product_stock_adjust.change_stock %></td>
        <td><%= product_stock_adjust.ext_prod_code %></td>
        <td><%= product_stock_adjust.barcode %></td>
        <td><%= product_stock_adjust.created_date %></td>
        <td><%= product_stock_adjust.emp_code %></td>
        <td><%= product_stock_adjust.emp_id %></td>
        <td><%= product_stock_adjust.name %></td>
        <td><%= product_stock_adjust.description %></td>
        <td><%= link_to 'Show', product_stock_adjust %></td>
        <td><%= link_to 'Edit', edit_product_stock_adjust_path(product_stock_adjust) %></td>
        <td><%= link_to 'Destroy', product_stock_adjust, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

