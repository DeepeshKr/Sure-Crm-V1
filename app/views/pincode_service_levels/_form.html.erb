<%= simple_form_for(@pincode_service_level) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :pincode %>
    <%= f.input :total_orders %>
    <%= f.input :total_value %>
    <%= f.input :last_ran_on %>
    <%= f.input :description %>
    <%= f.input :courier_id %>
    <%= f.input :time_to_deliver %>
    <%= f.input :cod_avail %>
    <%= f.input :distributor_avail %>
    <%= f.input :paid_order %>
    <%= f.input :paid_value %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
