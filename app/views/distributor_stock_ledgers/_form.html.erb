<%= simple_form_for(@distributor_stock_ledger) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :corporate_id %>
    <%= f.input :product_master_id %>
    <%= f.input :product_variant_id %>
    <%= f.input :product_list_id %>
    <%= f.input :prod %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :stock_change %>
    <%= f.input :ledger_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
