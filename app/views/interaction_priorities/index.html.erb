<h1>Listing interaction_priorities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sortorder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interaction_priorities.each do |interaction_priority| %>
      <tr>
        <td><%= interaction_priority.name %></td>
        <td><%= interaction_priority.sortorder %></td>
        <td><%= link_to 'Show', interaction_priority %></td>
        <td><%= link_to 'Edit', edit_interaction_priority_path(interaction_priority) %></td>
        <td><%= link_to 'Destroy', interaction_priority, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interaction priority', new_interaction_priority_path %>
