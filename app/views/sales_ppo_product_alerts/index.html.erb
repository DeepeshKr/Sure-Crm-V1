<p id="notice"><%= notice %></p>

<h1>Listing Sales Ppo Product Alerts</h1>

<table>
  <thead>
    <tr>
      <th>Product list</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sales_ppo_product_alerts.each do |sales_ppo_product_alert| %>
      <tr>
        <td><%= sales_ppo_product_alert.product_list_id %></td>
        <td><%= link_to 'Show', sales_ppo_product_alert %></td>
        <td><%= link_to 'Edit', edit_sales_ppo_product_alert_path(sales_ppo_product_alert) %></td>
        <td><%= link_to 'Destroy', sales_ppo_product_alert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

