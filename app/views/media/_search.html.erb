
<div class="row">
  <div class="col-xs-4">
  <div class="well well-sm">
  <div class="row">
    <%= bootstrap_form_tag url: media_path, :method => :GET, :enforce_utf8 => false do |f| %>
      <div class="col-xs-10">
          <%= text_field_tag(:telephone, @telephone,
          {:class => "form-control", :placeholder => "Search Telephone"})  %>
      </div>
      <div class="col-xs-2">
          <%= submit_tag "Ok", name: nil,  class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
  </div>
  </div>
  <div class="col-xs-3">
  <div class="well well-sm">
    <div class="row">
      <%= bootstrap_form_tag url: media_path, :method => :GET, :enforce_utf8 => false do |f| %>
      
      <div class="col-xs-9">
            <%= text_field_tag(:dnis, @dnis,
            {:class => "form-control", :placeholder => "Search DNI"})  %>
        </div>
        <div class="col-xs-3">
            <%= submit_tag "Ok", name: nil,  class: "btn btn-primary" %>
        </div>
      <% end %>
      </div>
    </div>
  </div>
 <div class="col-xs-4">
  <div class="well well-sm">
    <div class="row">
      <%= bootstrap_form_tag url: media_path, :method => :GET, :enforce_utf8 => false do |f| %>
      
      <div class="col-xs-10">
            <%= text_field_tag(:name, @dnis,
            {:class => "form-control", :value => @search,  :placeholder => "Search by Name"})  %>
        </div>
        <div class="col-xs-2">
            <%= submit_tag "Ok", name: nil,  class: "btn btn-primary" %>
        </div>
      <% end %>
      </div>
    </div>
  </div> 
  <div class="col-xs-1">
  <div class="well well-sm">
    
      <%= bootstrap_form_tag url: media_path, :method => :GET, :enforce_utf8 => false do |f| %>
     
      <%= hidden_field_tag :showall, @showall %>
           
           
       
            <%= submit_tag "All", name: nil,  class: "btn btn-primary" %>
        
      <% end %>
     
    </div>
  </div>
</div>