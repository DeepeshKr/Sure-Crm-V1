<h1>Listing product_warehouses</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Location name</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>Address3</th>
      <th>Landmark</th>
      <th>City</th>
      <th>Pincode</th>
      <th>State</th>
      <th>Country</th>
      <th>Telephone1</th>
      <th>Telephone2</th>
      <th>Fax</th>
      <th>Emailid</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_warehouses.each do |product_warehouse| %>
      <tr>
        <td><%= product_warehouse.location_name %></td>
        <td><%= product_warehouse.address1 %></td>
        <td><%= product_warehouse.address2 %></td>
        <td><%= product_warehouse.address3 %></td>
        <td><%= product_warehouse.landmark %></td>
        <td><%= product_warehouse.city %></td>
        <td><%= product_warehouse.pincode %></td>
        <td><%= product_warehouse.state %></td>
        <td><%= product_warehouse.country %></td>
        <td><%= product_warehouse.telephone1 %></td>
        <td><%= product_warehouse.telephone2 %></td>
        <td><%= product_warehouse.fax %></td>
        <td><%= product_warehouse.emailid %></td>
        <td><%= product_warehouse.description %></td>
        <td><%= link_to 'Show', product_warehouse %></td>
        <td><%= link_to 'Edit', edit_product_warehouse_path(product_warehouse) %></td>
        <td><%= link_to 'Destroy', product_warehouse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product warehouse', new_product_warehouse_path, class: "btn btn-primary" %>
