<p id="notice"><%= notice %></p>
<div class="row">
<div class="col-xs-4">
<h3>Recent Page Visits</h3>
</div>
<div class="col-xs-8">
<%= render 'search' %>
</div>
</div>


<table class="table table-hover">
  <thead>
    <tr>
      <th>Page Name</th>
      <th>Order</th>
     
      <th>Url</th>
      <th>Employee</th>
      <th>Est Secs</th>
      
    </tr>
  </thead>

  <tbody>
    <% @page_trails.each do |page_trail| %>
      <tr>
        <td><%= page_trail.name %></td>
        <td><%= page_trail.order_id %></td>
       
        <td><%= page_trail.url %></td>
        <td><%= page_trail.employee_id %></td>
        <td><%= page_trail.duration_secs %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

