<p id="notice"><%= notice %></p>
<div class="row">
<div class="col-xs-4">
<h3>Recent Page Visits</h3>
</div>
<div class="col-xs-8">
<%= render 'search' %>
</div>
</div>
 <br>
<div class="digg_pagination">
    <%= will_paginate @page_trails, :container => false %>
</div>
<br>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Date and Time</th>
      <th>Page Name</th>
      <th>Order</th>
     
      <th>Url</th>
      <th>Employee</th>
      
    </tr>
  </thead>

  <tbody>
    <% @page_trails.each do |page_trail| %>
      <tr>
        <td><%= (page_trail.created_at + 330.minutes).strftime('%d-%b-%y %H:%M %p') %></td>
        <td><%= page_trail.name %></td>
        <td><%= page_trail.order_id %></td>
       
        <td><%= page_trail.url %></td>
        <td><%= page_trail.employee.name if page_trail.employee %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<br>
<div class="digg_pagination">
    <%= will_paginate @page_trails, :container => false %>
</div>
<br>
