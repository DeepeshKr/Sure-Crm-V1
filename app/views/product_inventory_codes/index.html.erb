<h1>Listing product_inventory_codes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sortorder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_inventory_codes.each do |product_inventory_code| %>
      <tr>
        <td><%= product_inventory_code.name %></td>
        <td><%= product_inventory_code.sortorder %></td>
        <td><%= link_to 'Show', product_inventory_code %></td>
        <td><%= link_to 'Edit', edit_product_inventory_code_path(product_inventory_code) %></td>
        <td><%= link_to 'Destroy', product_inventory_code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product inventory code', new_product_inventory_code_path %>
