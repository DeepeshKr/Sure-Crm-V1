<p id="notice"><%= notice %></p>

<h1>Listing Distributor Stock Ledger Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Sort order</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @distributor_stock_ledger_types.each do |distributor_stock_ledger_type| %>
      <tr>
        <td><%= distributor_stock_ledger_type.name %></td>
        <td><%= distributor_stock_ledger_type.sort_order %></td>
        <td><%= distributor_stock_ledger_type.description %></td>
        <td><%= link_to 'Show', distributor_stock_ledger_type %></td>
        <td><%= link_to 'Edit', edit_distributor_stock_ledger_type_path(distributor_stock_ledger_type) %></td>
        <td><%= link_to 'Destroy', distributor_stock_ledger_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

