<div class="well well-sm">

  <%= @distributor_upload_order.description %> Last run at <%= @distributor_upload_order.last_ran_on.strftime("%d-%b-%Y %H:%M:%S") %>
<p>
  You can report for sales in each pincode <%= link_to "Check Orders in Pincode", "/pincode_orders" %>
</p>
</div>

<hr>
<div class="row">
 <%= bootstrap_form_tag url: transfer_order_path, :method => :GET, :enforce_utf8 => false do |f| %>
<div class="col-xs-8">
<%= text_field_tag(:order_id, nil, :class => "form-control", :required => "true", :placeholder => "Enter Order Id", :value => @order_id  )   %>
</div>

<div class="col-xs-2">
	<%= submit_tag "Check", name: nil, class: "btn btn-primary" %>
</div>
<% end %>

</div>
