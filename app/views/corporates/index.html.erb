<h1>Listing corporates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>Address3</th>
      <th>Landmark</th>
      <th>City</th>
      <th>Pincode</th>
      <th>State</th>
      <th>District</th>
      <th>Country</th>
      <th>Telephone1</th>
      <th>Telephone2</th>
      <th>Fax</th>
      <th>Website</th>
      <th>Salute1</th>
      <th>First name1</th>
      <th>Last name1</th>
      <th>Designation1</th>
      <th>Mobile1</th>
      <th>Emaild1</th>
      <th>Salute2</th>
      <th>First name2</th>
      <th>Last name2</th>
      <th>Designation2</th>
      <th>Mobile2</th>
      <th>Emailid2</th>
      <th>Salute3</th>
      <th>First name3</th>
      <th>Last name3</th>
      <th>Designation3</th>
      <th>Mobile3</th>
      <th>Emailid3</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @corporates.each do |corporate| %>
      <tr>
        <td><%= corporate.name %></td>
        <td><%= corporate.address1 %></td>
        <td><%= corporate.address2 %></td>
        <td><%= corporate.address3 %></td>
        <td><%= corporate.landmark %></td>
        <td><%= corporate.city %></td>
        <td><%= corporate.pincode %></td>
        <td><%= corporate.state %></td>
        <td><%= corporate.district %></td>
        <td><%= corporate.country %></td>
        <td><%= corporate.telephone1 %></td>
        <td><%= corporate.telephone2 %></td>
        <td><%= corporate.fax %></td>
        <td><%= corporate.website %></td>
        <td><%= corporate.salute1 %></td>
        <td><%= corporate.first_name1 %></td>
        <td><%= corporate.last_name1 %></td>
        <td><%= corporate.designation1 %></td>
        <td><%= corporate.mobile1 %></td>
        <td><%= corporate.emaild1 %></td>
        <td><%= corporate.salute2 %></td>
        <td><%= corporate.first_name2 %></td>
        <td><%= corporate.last_name2 %></td>
        <td><%= corporate.designation2 %></td>
        <td><%= corporate.mobile2 %></td>
        <td><%= corporate.emailid2 %></td>
        <td><%= corporate.salute3 %></td>
        <td><%= corporate.first_name3 %></td>
        <td><%= corporate.last_name3 %></td>
        <td><%= corporate.designation3 %></td>
        <td><%= corporate.mobile3 %></td>
        <td><%= corporate.emailid3 %></td>
        <td><%= corporate.description %></td>
        <td><%= link_to 'Show', corporate %></td>
        <td><%= link_to 'Edit', edit_corporate_path(corporate) %></td>
        <td><%= link_to 'Destroy', corporate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

